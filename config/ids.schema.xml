		<column name="ca_usucreado" type="varchar" required="true"   />

        <foreign-key foreignTable="ids.tb_ids">
			<reference local="ca_id" foreign="ca_id"/>
		</foreign-key>

        <foreign-key foreignTable="ids.tb_tipodocumentos">
			<reference local="ca_idtipo" foreign="ca_idtipo"/>
		</foreign-key>
	</table>

    <table name="ids.tb_evaluacion" phpName="IdsEvaluacion">
		<column name="ca_idevaluacion" type="integer" required="true" primaryKey="true" autoIncrement="true" />
		<id-method-parameter value="ids.tb_evaluacion_id"/>
        <column name="ca_id" type="integer" required="true"  />
        <column name="ca_concepto" type="varchar"   />
        <column name="ca_tipo" type="varchar"   />
        <column name="ca_fchevaluacion" type="date" required="true"  />
		<column name="ca_fchcreado" type="timestamp" required="true"/>
		<column name="ca_usucreado" type="varchar" required="true"   />

        <foreign-key foreignTable="ids.tb_ids">
			<reference local="ca_id" foreign="ca_id"/>
		</foreign-key>
	</table>

    <table name="ids.tb_criterios" phpName="IdsCriterio">
		<column name="ca_idcriterio" type="integer" required="true" primaryKey="true" autoIncrement="true" />
		<id-method-parameter value="ids.tb_criterios_id"/>
        <column name="ca_tipo" type="varchar" required="true"  />
        <column name="ca_tipocriterio" type="varchar" required="true"  />
        <column name="ca_criterio" type="varchar"   />
        <column name="ca_activo" type="boolean"   />
		<column name="ca_fchcreado" type="timestamp" required="true"/>
		<column name="ca_usucreado" type="varchar" required="true"   />
        <column name="ca_fchactualizado" type="timestamp" required="true"/>
		<column name="ca_usuactualizado" type="varchar" required="true"   />
       
	</table>


   <table name="ids.tb_evaluacionxcriterio" phpName="IdsEvaluacionxCriterio">
		<column name="ca_idevaluacion" type="integer" required="true" primaryKey="true"/>
        <column name="ca_idcriterio" type="varchar" required="true"  primaryKey="true" />
        <column name="ca_valor" type="numeric" required="true"  />
        <column name="ca_ponderacion" type="numeric"   />
        <column name="ca_observaciones" type="varchar"   />		

        <foreign-key foreignTable="ids.tb_evaluacion">
			<reference local="ca_idevaluacion" foreign="ca_idevaluacion"/>
		</foreign-key>

        <foreign-key foreignTable="ids.tb_criterios">
			<reference local="ca_idcriterio" foreign="ca_idcriterio"/>
		</foreign-key>

	</table>

    <table name="ids.tb_eventos" phpName="IdsEvento">
		<column name="ca_idevento" type="integer" required="true" primaryKey="true" autoIncrement="true" />
		<id-method-parameter value="ids.tb_eventos_id"/>
        <column name="ca_id" type="integer" required="true" />
        <column name="ca_evento" type="varchar" />
        <column name="ca_referencia" type="varchar"   />
        <column name="ca_tipo" type="varchar"   />
        <column name="ca_fchcreado" type="timestamp" required="true"/>
		<column name="ca_usucreado" type="varchar" required="true"   />
        <foreign-key foreignTable="ids.tb_ids">
			<reference local="ca_id" foreign="ca_id"/>
		</foreign-key>
	</table>


</database>
