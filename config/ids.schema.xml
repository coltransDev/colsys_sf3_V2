		<column name="ca_fchcreado" type="timestamp" required="true" />
		<column name="ca_usucreado" type="varchar" required="true"   />
        <column name="ca_fchactualizado" type="timestamp"   />
		<column name="ca_usuactualizado" type="varchar"   />
       
	</table>

	<table name="ids.tb_documentos" phpName="IdsDocumento">
		<column name="ca_iddocumento" type="integer" required="true" primaryKey="true" autoIncrement="true" />
		<id-method-parameter value="ids.tb_documentos_id"/>
        <column name="ca_id" type="integer" required="true"  />
		<column name="ca_idtipo" type="integer" required="true"  />
        <column name="ca_ubicacion" type="varchar" />
		<column name="ca_fchinicio" type="date" required="true"  />
        <column name="ca_fchvencimiento" type="date" />
        <column name="ca_observaciones" type="varchar"   />
